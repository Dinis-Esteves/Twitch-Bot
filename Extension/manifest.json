{
    "manifest_version": 2,
    "name": "Video Title getter",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "description": "Get the Url from the current Tab",
    "permissions": ["activeTab", "storage", "<all_urls>","tabs"],
    "externally_connectable": {
      "matches": ["<all_urls>"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  }
}
